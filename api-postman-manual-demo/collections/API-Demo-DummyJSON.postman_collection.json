							"listen": "test",
							"script": {
								"exec": [
									"//Validate Each Item in Array\r",
									"//Each product must have id, title, and price.\r",
									"pm.test(\"Validate Each Item in Array\", function () {\r",
									"    const jsonProducts = pm.response.json().products;\r",
									"    pm.response.to.have.status(200);\r",
									"    jsonProducts.forEach(product => {\r",
									"        pm.expect(product).to.have.property('id');\r",
									"        pm.expect(product).to.have.property('title');\r",
									"        pm.expect(product).to.have.property('price');\r",
									"        pm.expect(product.price).to.have.above(0);\r",
									"    })\r",
									"});"
								],
