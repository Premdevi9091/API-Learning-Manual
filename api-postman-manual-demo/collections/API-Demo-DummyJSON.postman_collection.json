		{
			"name": "Assertions Example",
			"item": [
				{
					"name": "API should return products array and pagination metadata",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"//API should return products array and pagination metadata.\r",
									"pm.test(\"API should return products array and pagination metadata\", function(){\r",
									"    const json = pm.response.json();\r",
									"    pm.response.to.have.status(200);\r",
									"    pm.expect(json).to.have.property(\"products\");\r",
									"    pm.expect(json).to.have.property(\"total\");\r",
									"    pm.expect(json).to.have.property(\"limit\");\r",
									"    pm.expect(json).to.have.property(\"skip\");\r",
									"    pm.expect(json.products).to.be.an(\"array\");\r",
									"    \r",
									"});"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
